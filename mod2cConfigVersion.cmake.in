# Check PACKAGE_FIND_VERSION against built version @CVF_PACKAGE_VERSION@
#
# Set PACKAGE_VERSION_COMPATIBLE to TRUE iff:
#    PACKAGE_FIND_VERSION is not greater than @PQACKAGE_VERSION, and
#    PACKAGE_FIND_VERSION_MAJOR has the same major version number.
#
# Set PACKAGE_VERSION_EXACT to TRUE iff PACKAGE_FIND_VERSION is an
# exact match.

if("@CVF_PACKAGE_VERSION@" VERSION_LESS "${PACKAGE_FIND_VERSION}" )
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
else()
  set(VERSION_MAJOR "@CVF_PACKAGE_VERSION@")
  if("@CVF_PACKAGE_VERSION@" MATCHES "^([0-9]+)\\.")
    set(VERSION_MAJOR "${CMAKE_MATCH_1}")
  endif()

  if("${PACKAGE_FIND_VERSION_MAJOR}" STREQUAL "${VERSION_MAJOR}")
    set(PACKAGE_VERSION_COMPATIBLE TRUE)
  else()
    set(PACKAGE_VERSION_COMPATIBLE FALSE)
  endif()

  if("${PACKAGE_FIND_VERSION}" STREQUAL "@CVF_PACKAGE_VERSION@")
      set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()

